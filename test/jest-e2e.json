{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testTimeout": 150000,
  "testRegex": ".e2e-spec.ts$",
  "verbose": true,
  "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@infrastructures/(.*)$": "<rootDir>/src/infrastructure/$1",
      "^@helpers/(.*)$": "<rootDir>/src/helpers/$1"
  }
}